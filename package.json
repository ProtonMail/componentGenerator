{
  "name": "component-generator",
  "version": "1.4.2",
  "description": "CLI to create new components for: Angular Front-End for ProtonMail",
  "main": "index.js",
  "bin": {
    "appComponent": "index.js"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "dedent": "^0.7.0",
    "execa": "^1.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.4.0",
    "gulp-tap": "^1.0.1",
    "inquirer": "^6.1.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "natives": "^1.1.4",
    "prettier": "^1.14.2"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2"
  },
  "scripts": {
    "bind:bash": "echo \"\nalias appComponent='$(pwd)/index.js'\" >> ~/.bashrc",
    "bind:zsh": "echo \"\nalias appComponent='$(pwd)/index.js'\" >> ~/.zshrc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pretty": "prettier --write index.js lib/**/*.js lib/*.js",
    "precommit": "lint-staged",
    "postversion": "git push --tags"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT"
}
