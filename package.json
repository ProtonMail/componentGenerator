{
  "name": "component-generator",
  "version": "1.3.2",
  "description": "CLI to create new components for: Angular Front-End for ProtonMail",
  "main": "index.js",
  "bin": {
    "appComponent": "index.js"
  },
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-tap": "^0.1.3",
    "inquirer": "^1.1.3",
    "inquirer-autocomplete-prompt": "^0.12.0",
    "prettier": "^1.9.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0"
  },
  "scripts": {
    "bind:bash": "echo \"\nalias appComponent='$(pwd)/index.js'\" >> ~/.bashrc",
    "bind:zsh": "echo \"\nalias appComponent='$(pwd)/index.js'\" >> ~/.zshrc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pretty": "prettier --arrow-parens=always --single-quote --tab-width=4 --print-width=150 --prose-wrap=never --write index.js lib/**/*.js lib/*.js",
    "precommit": "lint-staged",
    "postversion": "git push --tags"
  },
  "lint-staged": {
    "*.js": [
      "prettier --arrow-parens=always --single-quote --tab-width=4 --print-width=150 --no-prose-wrap --write",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT"
}
